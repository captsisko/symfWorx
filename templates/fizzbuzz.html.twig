{% extends 'base.html.twig' %}

{% block title %} FizzBuZZzzzz... {% endblock %}

{% block body %}
    {% for i in 1..20 %}
        {% if (i % 3 == 0) and (i % 5 == 0) %}
            FizzBuzz
        {% elseif i % 5 == 0 %}
            Buzz
        {% elseif i % 3 == 0 %}
            Fizz
        {% else %}
            {{ i }}
        {% endif %}
    {% endfor %}
{% endblock %}